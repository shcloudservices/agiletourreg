{% extends 'SHCloudRegisterBundle:Register:base.html.twig' %}
{% block content %}Registro de ponente de {{ciudad}}

<form action="{{ path('registro_ponente') }}" method="post" {{ form_enctype(form) }}>
    {{ form_errors(form) }}

        <label for="fos_user_registration_form_email">Correo Electr√≥nico</label>{{ form_widget(form.email) }}
    {{ form_row(form.nombre) }}
    {{ form_row(form.tipo) }}

    {{ form_row(form.presentacion.titulo) }}
    {{ form_row(form.presentacion.resumen) }}
    {{ form_row(form.presentacion.laminas) }}
        
    {{ form_rest(form) }}

    <input type="submit" />
</form>

{% endblock %}